        "sections.temoignages",
        "sections.pied-de-page",
        "sections.galerie",
        "sections.fonctionnalites",
        "sections.entete",
        "sections.contact",
        "sections.a-propos",
        "sections.faq",
        "sections.marquee",
        "sections.bloc-texte-image",
        "sections.bloc-element-image",
        "sections.formulaire-de-contant",
        "sections.liste-blog"
      ]
    },
    "seo": {
      "type": "component",
      "component": "utils.seo",
      "repeatable": false,
      "required": true
    }
  }
}
