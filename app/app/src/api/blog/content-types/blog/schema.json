{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Blog"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "titre": {
      "type": "string",
      "required": true
    },
    "description": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      },
      "required": true
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "auteur": {
      "type": "string",
      "required": true
    },
    "profile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "sections": {
      "type": "dynamiczone",
      "components": [
        "sections.bloc-element-image",
        "sections.bloc-texte-image"
      ]
    },
    "seo": {
      "type": "component",
      "component": "utils.seo",
      "repeatable": false,
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "titre"
    }
  }
}
